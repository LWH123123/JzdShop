<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:descendantFocusability="blocksDescendants">


    <TextView
        android:id="@+id/tv_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="12/11"
        android:textColor="@color/textColor"
        android:textSize="@dimen/px14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px4dp"
        android:paddingBottom="@dimen/px20dp"
        android:text="10.00"
        android:textColor="@color/textColor"
        android:textSize="@dimen/px12sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_data"
        app:layout_constraintStart_toStartOf="@+id/tv_data"
        app:layout_constraintTop_toBottomOf="@+id/tv_data" />


    <View
        android:id="@+id/view1"
        android:layout_width="1dp"
        android:layout_height="@dimen/px30dp"
        android:layout_marginStart="@dimen/px56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_other"
        android:layout_width="@dimen/px8dp"
        android:layout_height="@dimen/px8dp"
        android:background="@drawable/shape_radius_round_gray"
        android:textColor="@color/white"
        app:layout_constraintEnd_toStartOf="@id/view_sign"
        app:layout_constraintStart_toEndOf="@id/view1"
        app:layout_constraintTop_toTopOf="@+id/tv_data" />


    <View
        android:id="@+id/view"
        android:layout_width="0.8dp"
        android:layout_height="@dimen/px0dp"
        android:background="@color/level5text"
        android:paddingBottom="@dimen/px10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/view_sign"
        app:layout_constraintStart_toEndOf="@+id/view1"
        app:layout_constraintTop_toBottomOf="@+id/tv_other" />

    <TextView
        android:id="@+id/tv_frist"
        android:layout_width="@dimen/px16dp"
        android:layout_height="@dimen/px16dp"
        android:background="@drawable/radius_round"
        android:textColor="@color/white"
        app:layout_constraintEnd_toStartOf="@id/view_sign"
        app:layout_constraintStart_toEndOf="@id/view1"
        app:layout_constraintTop_toTopOf="@+id/tv_data" />

    <View
        android:id="@+id/view_sign"
        android:layout_width="1dp"
        android:layout_height="30dp"
        android:layout_marginStart="@dimen/px100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/address"
        android:layout_width="@dimen/px0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/px20dp"
        android:layout_marginEnd="@dimen/px14dp"
        android:autoLink="all"
        android:lineSpacingExtra="@dimen/px3dp"
        android:paddingBottom="@dimen/px20dp"
        android:textColor="@color/textColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_other"
        app:layout_constraintTop_toTopOf="@+id/tv_data" />
</android.support.constraint.ConstraintLayout>
