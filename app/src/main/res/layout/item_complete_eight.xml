<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="@dimen/px60dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px1dp"
        android:background="@color/level5text"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_xing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/px16dp"
        android:visibility="invisible"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/textColor"
        android:textSize="@dimen/px15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_xing"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/rl_selectone"
        android:layout_width="@dimen/px120dp"
        android:layout_height="@dimen/px26dp"
        android:layout_marginStart="@dimen/px90dp"
        android:background="@color/level9text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.517">

        <TextView
            android:id="@+id/tv_valueone"
            android:text="开始"
            android:textSize="@dimen/px12sp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/px6dp"
            android:gravity="center_vertical"
             />

        <ImageView
            android:layout_width="@dimen/px16dp"
            android:layout_height="@dimen/px16dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/px4dp"
            android:layout_marginRight="@dimen/px6dp"
            android:layout_marginBottom="@dimen/px4dp"
            android:src="@mipmap/unfold" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_selecttwo"
        android:layout_width="@dimen/px120dp"
        android:layout_height="26dp"
        android:layout_marginEnd="10dp"
        android:background="@color/level9text"
        app:layout_constraintBottom_toBottomOf="@+id/rl_selectone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rl_selectone"
        app:layout_constraintVertical_bias="0.6">
        <TextView
            android:id="@+id/tv_valuetwo"
            android:text="结束"
            android:textSize="@dimen/px12sp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/px6dp"
            android:gravity="center_vertical"
             />

        <ImageView
            android:layout_marginTop="@dimen/px4dp"
            android:layout_width="@dimen/px16dp"
            android:layout_height="@dimen/px16dp"
            android:layout_marginBottom="@dimen/px4dp"
            android:layout_marginRight="@dimen/px6dp"
            android:layout_alignParentRight="true"
            android:src="@mipmap/unfold" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="至"
        android:layout_marginStart="@dimen/px6dp"
        android:layout_marginEnd="@dimen/px6dp"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toBottomOf="@+id/rl_selectone"
        app:layout_constraintEnd_toStartOf="@+id/rl_selecttwo"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toEndOf="@+id/rl_selectone"
        app:layout_constraintTop_toTopOf="@+id/rl_selectone"
        app:layout_constraintVertical_bias="0.428" />

</android.support.constraint.ConstraintLayout>