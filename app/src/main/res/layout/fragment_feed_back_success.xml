<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="feedbacksuccessVM"
            type="com.jzd.jzshop.ui.mine.feedback.FeedBackSuccessViewModel" />

    </data>

<android.support.constraint.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jzd.jzshop.ui.mine.feedback.FeedBackSuccessFragment">

    <include
        android:id="@+id/asc_include_toolbar"
        layout="@layout/layout_toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:toolbarViewModel="@{feedbacksuccessVM.toolbarViewModel}" />

    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="@dimen/px66dp"
        android:layout_height="@dimen/px66dp"
        android:src="@mipmap/paysuccess"
        android:layout_marginTop="@dimen/px60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/asc_include_toolbar" />


    <TextView
        android:id="@+id/textView68"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px14dp"
        android:text="@string/feedback_success"
        android:textColor="@color/textColor"
        android:textSize="@dimen/px20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView56"
        app:layout_constraintStart_toStartOf="@+id/imageView56"
        app:layout_constraintTop_toBottomOf="@+id/imageView56" />

    <TextView
        android:layout_width="@dimen/px260dp"
        android:layout_height="wrap_content"
        android:text="@string/feedback_hint"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/px16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView68" />


</android.support.constraint.ConstraintLayout>
</layout>
